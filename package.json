{
  "name": "json-builder",
  "version": "0.0.1",
  "description": "Build a JSON file from a table",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.electron.jsonbuilder",
    "productName": "JSON Builder",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "installerIcon": "assets/icon.ico",
      "uninstallerIcon": "assets/icon.ico",
      "runAfterFinish": true
    }
  },
  "repository": "https://github.com/JakeAve/JSONBuilder.git",
  "keywords": [],
  "author": "Jacob Avery",
  "devDependencies": {
    "electron": "^4.1.3",
    "electron-builder": "^20.39.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.0",
    "npm": "^6.13.4",
    "popper.js": "^1.15.0"
  }
}
